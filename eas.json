{
  "cli": {
    "version": ">= 10.2.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL_UFLOW": "https://uflow.aracena.dev",
        "APP_VARIANT": "preview"
      },
      "channel": "preview"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
